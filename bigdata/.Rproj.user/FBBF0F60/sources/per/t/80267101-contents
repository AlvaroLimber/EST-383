# Símbolos Lógicos
5 == 6  # igualdad
5^2 == 25

5 != 6 # desigualdad
25 != 25

!(5>10) # negación

(5==5) & (5>6) # Y lógico
(5==5) & (5<6) # Y lógico

(5==5) | (5>6) # ó lógico
(5==5) | (5<6) # ó lógico
(5!=5) | (5>6) # ó lógico
